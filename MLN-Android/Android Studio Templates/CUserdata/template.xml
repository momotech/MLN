<?xml version="1.0"?>
<template
    format="5"
    revision="3"
    name="Userdata (Native)"
    description="Creates a Userdata for Lua"
    minApi="7"
    minBuildApi="8">

    <category value="MLN" />

    <parameter
        id="ClassName"
        name="Java Class Name"
        type="string"
        constraints="class|nonempty|unique"
        default=""
        help="The name of the java class to create" />

    <parameter
        id="LuaClassName"
        name="Lua Class Name"
        type="string"
        constraints="nonempty"
        default=""
        help="The name of the lua class to create" />

    <parameter
        id="WrapClass"
        name="Wraped Class Name"
        type="string"
        constraints="class|nonempty|unique"
        default=""
        help="The name of the java class to Wraped in" />

    <globals file="globals.xml.ftl" />
    <execute file="recipe.xml.ftl" />

</template>
